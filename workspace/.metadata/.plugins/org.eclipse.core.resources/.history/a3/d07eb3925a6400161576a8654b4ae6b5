import java.io.IOException;
import java.net.Socket;
import java.net.UnknownHostException;


public class NetClient {
	
	public void connect(String ip, int port) {
		try {
			Socket s = new Socket(ip, port);
			System.out.println("");
		} catch (UnknownHostException e) {
			e.printStackTrace();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
